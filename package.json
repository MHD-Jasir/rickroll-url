{
  "name": "fliplink-react",
  "version": "1.0.0",
  "description": "The 50/50 Surprise URL Shortener - React Edition",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "node server.js",
    "setup": "npm install && cd client && npm install",
    "heroku-postbuild": "npm run setup && npm run build"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "node-localstorage": "^3.0.5",
    "nanoid": "^3.3.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": ["url-shortener", "rickroll", "react", "meme"],
  "author": "FlipLink Team",
  "license": "MIT"
}